    "yargs": "^11.0.0"
  },
  "scripts": {
    "pre": "npm-run-all generate:*",
    "prebuild": "yarn install && npm-run-all clean:* generate:* copy:images",
    "prewatch": "npm-run-all clean:assets generate:* copy:images",
    "clean:assets": "rm -rf ./public/*",
    "clean:dist": "rm -rf ./dist/*",
    "clean:docs": "rm -rf ./docs/*",
    "generate:js": "node ./scripts/js.js",
    "generate:sass": "node ./scripts/sass.js",
    "generate:vars": "node ./scripts/sass-vars.js",
    "build:js": "NODE_ENV=production webpack --config scripts/webpack.config.babel.js",
    "build:styleguide": "NODE_ENV=production fractal build",
    "build:sass": "NODE_ENV=production node-sass-chokidar ./src/scss/style-guide-theme-overrides.scss -o ./docs/css/",
    "watch:js": "webpack --config scripts/webpack.config.babel.js -w",
    "watch:sass": "node-sass-chokidar ./src/scss/style-guide-theme-overrides.scss -o ./public/css/ --watch",
    "watch:styleguide": "fractal start --sync",
    "copy:images": "mkdir -p ./public && cp -r ./src/images ./public/images",
    "copy:sass": "node ./scripts/copy-sass-to-dist.js",
    "build": "NODE_ENV=production npm-run-all build:*",
    "watch": "npm-run-all -p watch:*",
    "post": "",
    "postbuild": "npm-run-all copy:sass",
    "start": "npm run watch"
  },
  "dependencies": {
    "@ungap/url-search-params": "^0.1.2",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.0",
    "break": "^1.0.2",
    "croppie": "^2.6.4",
    "html-validator-cli": "^4.0.0",
    "inputmask": "^4.0.0",
    "npm": "^6.4.1",
    "password-validator": "^4.1.1",
    "postcss-cli": "^5.0.0",
    "serve": "^6.5.5",
    "slick-carousel": "^1.8.1",
    "stickybits": "^3.3.7",
    "yarn": "^1.10.1"
  }
}
