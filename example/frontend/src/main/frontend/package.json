{
  "name": "aem-patterns-example-frontend-build",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "",
  "author": "ICF Olson",
  "license": "none",
  "private": true,
  "config": {
    "rootFolderName": "frontend",
    "styleguideRootPath": "/aem-patterns-example/",
    "projectTitle": "AEM Patterns Example"
  },
  "devDependencies": {
    "@epegzz/sass-vars-loader": "^3.2.3",
    "@frctl/fractal": "1.2.1-beta.1",
    "@frctl/mandelbrot": "^1.2.1",
    "autoprefixer": "^8.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.4.1",
    "cross-env": "^6.0.3",
    "css-loader": "^0.28.11",
    "extract-loader": "^2.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "fs-extra": "^6.0.0",
    "glob-fs": "^0.1.7",
    "globby": "^8.0.1",
    "handlebars-helpers": "^0.10.0",
    "handlebars-layouts": "^3.1.4",
    "jquery": "^3.3.1",
    "js-cookie": "^2.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "moment": "^2.24.0",
    "node-sass": "^4.8.3",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "popper.js": "^1.14.3",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.0.1",
    "semver": "^5.5.0",
    "shx": "^0.3.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "yargs": "^11.0.0"
  },
  "scripts": {
    "prebuild-fractal": "npm-run-all clean:* generate:* copy:images",
    "prebuild": "npm-run-all clean:* generate:*",
    "prewatch": "npm-run-all clean:* generate:* copy:images",
    "clean:assets": "shx rm -rf ./public/*",
    "clean:dist": "shx rm -rf ./dist/*",
    "clean:docs": "shx rm -rf ./docs/*",
    "generate:js": "node ./scripts/js.js",
    "generate:sass": "node ./scripts/sass.js",
    "generate:vars": "node ./scripts/sass-vars.js",
    "build:js": "cross-env NODE_ENV=production webpack --config scripts/webpack.config.babel.js",
    "build:styleguide": "cross-env NODE_ENV=production fractal build",
    "build:sass": "cross-env NODE_ENV=production node-sass-chokidar ./src/scss/overrides -o ./docs/css/",
    "watch:js": "webpack --config scripts/webpack.config.babel.js -w",
    "watch:sass": "node-sass-chokidar ./src/scss/overrides -o ./public/css/ --watch",
    "watch:styleguide": "fractal start --sync",
    "copy:images": "shx mkdir -p ./public ./src/images && shx cp -r ./src/images ./public/images",
    "copy:sass": "node ./scripts/copy-sass-to-dist.js",
    "build-fractal": "cross-env NODE_ENV=production npm-run-all build:*",
    "build": "cross-env NODE_ENV=production npm-run-all build:js",
    "watch": "npm-run-all -p watch:*",
    "postbuild-fractal": "npm-run-all copy:sass",
    "start": "npm run watch"
  },
  "dependencies": {
    "@ungap/url-search-params": "^0.1.2",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.3.1",
    "html-validator-cli": "^4.0.0",
    "inputmask": "^4.0.0",
    "npm": "^6.4.1",
    "postcss-cli": "^5.0.0",
    "serve": "^6.5.5",
    "slick-carousel": "^1.8.1",
    "stickybits": "^3.3.7",
    "yarn": "^1.10.1"
  }
}
